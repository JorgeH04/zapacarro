<section id="contact">
    <div class="container">
        <div class="row">
            <div class="map">
                <iframe
                    src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d6048.653683487531!2d-74.0042128!3d40.7108209!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c24fa5d33f083b%3A0xc80b8f06e177fe62!2sNew+York%2C+NY%2C+USA!5e0!3m2!1sen!2sbd!4v1554542158131!5m2!1sen!2sbd"
                    allowfullscreen></iframe>
            </div>
            <div class="col-lg-3"></div>
            <div class="col-lg-9">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="row mx-0">
                            <div class="col-lg-4 col-md-4">
                                <div class="contact-item pl-lg-3">
                                    <h4>Our Location</h4>
                                    <p>1234, Park Street Avenue</p>
                                    <p>Newyork city, North America.</p>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4">
                                <div class="contact-item">
                                    <h4>Our Email</h4>
                                    <p class="minus-top">bellaspacenter@gmail.com</p>
                                    <p>sandtheraphycenter@gmail.com</p>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4">
                                <div class="contact-item">
                                    <h4>Our Website</h4>
                                    <p class="minus-top">www.bellasapcenter.com</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 m-auto">
                    <div class="form">
                        <form method="post" class="appointment-form">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-12">
                                    <label>Name</label>
                                    <input type="text" class="form-control">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-6 col-md-6 col-12">
                                    <label>Email</label>
                                    <input type="email" class="form-control">
                                </div>
                                <div class="col-lg-6 col-md-6 col-12">
                                    <label>Phone Number</label>
                                    <input type="tel" class="form-control">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-6 col-md-6 col-12">
                                    <label>No. of Person</label>
                                    <input type="text" class="form-control sagor-m-0 noor-m-0">
                                </div>
                                <div class="col-lg-6 col-md-6 col-12">
                                    <label>Appointment Date</label>
                                    <input type="date" class="form-control sagor-m-0">
                                </div>
                            </div>
                            <div class="ph-btn text-center">
                                <button type="submit" class="abs-reflect">Send Message</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

